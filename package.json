{
  "name": "slidev-theme-Mobile-farsi-youtube",
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^51.4.0"
  },
  "devDependencies": {
    "@slidev/cli": "^51.4.0"
  },
  "//": "Learn more: https://sli.dev/guide/write-theme.html",
  "slidev": {
    "colorSchema": "both",
    "defaults": {
      "fonts": {
        "sans": "Vazirmatn",
        "mono": "Fira Code"
      },
      "aspectRatio": "9/16",
      "canvasWidth":720,
      "canvasHeight":1280,
      "htmlAttrs": {
        "dir": "rtl",
        "lang": "fa"
      }
    }
  }
}